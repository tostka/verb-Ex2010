                        # 5:15 PM 4/22/2025 on CMW, have to patch version to Ex2016
                        #if($env:userdomain -eq 'CMW'){
                        if($TenOrg -eq 'CMW'){
                            if($pltCcX10.keys -contains 'Version'){
