                    PS>  $logging = $True ;
                    PS>  $sBnr="#*======v START PASS:$($ScriptBaseName) v======" ;
                    PS>  $smsg= "$($sBnr)" ;
                    PS>  if ($logging) { Write-Log -LogContent $smsg -Path $logfile -useHost -Level Info } ; #Error|Warn
