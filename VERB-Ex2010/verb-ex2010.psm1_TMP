                        else{ write-host -foregroundcolor green "$((get-date).ToString('HH:mm:ss')):$($smsg)" } ;
                        <### CALLS ARE IN FORM: (cred$($tenorg))
                        $pltRX10 = @{
                            Credential = (Get-Variable -name "cred$($tenorg)OP" ).value ;
                            #verbose = $($verbose) ;
