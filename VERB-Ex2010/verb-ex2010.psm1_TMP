        PS>         } ; 
        PS>         if(-not $ret_CcOPSvcs.AdGcFwide){
        PS>             $smsg += "`n----->$($ACtionCommand): MISSING .AdGcFwide GC!:`n((Get-ADDomainController -Discover -Service GlobalCatalog).hostname):326) " ; 
