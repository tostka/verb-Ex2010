                    if($verbose){if ($logging) { Write-Log -LogContent $smsg -Path $logfile -useHost -Level VERBOSE } 
                    else{ write-verbose "$((get-date).ToString('HH:mm:ss')):$($smsg)" } ; } ; 
                    $DC | write-output  ;
                } else {
                    write-host -foregroundcolor yellow  "NO DCS RETURNED BY GET-GCFAST()!";
                    write-output $false ;
                } ;
            }  ; 
        } ; 
